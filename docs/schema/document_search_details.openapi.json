openapi: 3.1.0
info:
  title: Document Search Details Schema
  version: '1.0.0'
components:
  schemas:
    DocumentSearchDetails:
      type: object
      description: Details about a document search, including filters and suggestions.
      properties:
        filters:
          type: array
          items:
            $ref: '#/components/schemas/Filter'
          description: A list of filter objects provided by the API.
        suggestions:
          type: array
          items:
            $ref: '#/components/schemas/Suggestion'
          description: A list of suggestion objects provided by the API.
      required:
        - filters
        - suggestions
