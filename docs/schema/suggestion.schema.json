{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Suggestion",
  "type": "object",
  "properties": {
    "text": {
      "type": "string",
      "description": "The suggested text."
    },
    "url": {
      "type": "string",
      "format": "uri",
      "description": "The URL associated with the suggestion."
    }
  },
  "required": [
    "text",
    "url"
  ]
}